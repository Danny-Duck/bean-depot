<div>
  <h1>users</h1>
  <% if @users %>
    <% @users.each do |user| %>
      <div><%= user.fname + " " + user.lname %></div>
      <div><%= user.email %></div>
    <% end %> 
  <% else %>
    <p>there aren't any users yet!</p>
  <% end %>
</div>